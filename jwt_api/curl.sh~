#!/bin/bash

# Sign In Solution
curl -d "email=qwerqwer@qwer.qwer&password=qwerqwer" -H "Content-Type: application/x-www-form-urlencoded" -X POST http://localhost:3000/v1/users

# Login Solution
curl -d "user=qwerqwer@qwer.qwer&password=qwerqwer" -H "Content-Type: application/x-www-form-urlencoded" -X POST http://localhost:3000/v1/users/login

# Get User